{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/jsonapi/(.*)": "<rootDir>/../libs/jsonapi/src/$1",
    "@app/jsonapi": "<rootDir>/../libs/jsonapi/src",
    "@app/json-api-test/(.*)": "<rootDir>/../libs/json-api-test/src/$1",
    "@app/json-api-test": "<rootDir>/../libs/json-api-test/src",
    "@app/json-api/(.*)": "<rootDir>/../libs/json-api/src/$1",
    "@app/json-api": "<rootDir>/../libs/json-api/src"
  }
}